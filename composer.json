{
    "name": "hjerichen/dbunit",
    "description": "An alternative to phpunit/dbunit.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "symfony/yaml": "^6.0",
        "sebastian/comparator": "^4.0 | ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "vimeo/psalm": "^5.12",
        "psalm/plugin-phpunit": "^0.18.4"
    },
    "autoload": {
        "psr-4": {
            "HJerichen\\DBUnit\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HJerichen\\DBUnit\\Tests\\": "./tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Heiko Jerichen",
            "email": "heiko@jerichen.de"
        }
    ]
}
